{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "tns-sandbox": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "amway"
    }
  },
  "defaultProject": "tns-sandbox",
  "cli": {
    "defaultCollection": "@nativescript/schematics",
    "analytics": false
  }
}
